{
    "project_name": "flask_project",
    "project_short_description": "This is a Flask project template generated with Cookiecutter.",
    "docker_image_backend": "{{ cookiecutter.project_name|replace('_','-') }}",
    "python_version": ["3.13", "3.12", "3.11", "3.10", "3.8", "3.6"],
    "host": "0.0.0.0",
    "port": "5000",
    "base_url": "/services/{{cookiecutter.project_name}}",
    "secret_key": "_secret_key_to_replace_",
    "token_secret_key": "replace_token_secret_key",
    "backend_cors_origins": "http://localhost,http://localhost:{{ cookiecutter.port }}",
    "use_db": ["yes", "no"],
    "postgres_user": "{% if cookiecutter.use_db == 'yes' %}postgres{% else %}null{% endif %}",
    "postgres_password": "{% if cookiecutter.use_db == 'yes' %}postgres{% else %}null{% endif %}",
    "postgres_host": [
        "{% if cookiecutter.use_db == 'yes' %}localhost{% else %}null{% endif %}",
        "{% if cookiecutter.use_db == 'yes' %}postgres{% else %}null{% endif %}"
    ],
    "postgres_port": "{% if cookiecutter.use_db == 'yes' %}5432{% else %}null{% endif %}",
    "postgres_db": "{% if cookiecutter.use_db == 'yes' %}{{ cookiecutter.project_name }}_db{% else %}null{% endif %}"
}