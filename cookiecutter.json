{
    "project_name": "flask_project",
    "short_description_project": "This is a Flask project template generated with Cookiecutter.",
    "docker_image_backend": "{{ cookiecutter.project_name|replace('_','-') }}",
    "python_version": ["3.13", "3.12", "3.11", "3.10", "3.8", "3.6"],
    "host": "0.0.0.0",
    "port": "5000",
    "base_url": "/services/{{cookiecutter.project_name}}",
    "token_secret_key": "replace_token_secret_key",
    "backend_cors_origins": "http://localhost,http://localhost:{{ cookiecutter.port }}",
    "postgres_user": "postgres",
    "postgres_password": "postgres",
    "postgres_host": "localhost",
    "postgres_port": "5432",
    "postgres_db": "{{ cookiecutter.project_name }}_db"
}